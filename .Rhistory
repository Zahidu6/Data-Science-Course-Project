library(WDI)
vars <- c("NY.GDP.PCAP.KD", "SP.DYN.IMRT.IN", "DT.ODA.MULT.KD")
wdi_df <- WDI(indicator = vars, start = 1980, end = 2010, extra = TRUE) %>%
tbl_df()
ggplot(data = filter(wdi_df, year == 2010)) +
geom_text(aes(x = log(gdp_percap), y = inf_mort, label = iso3c), family = "mono", size = 2) +
xlab("Log(GDP Per Capita)") + ylab("Infant Mortality")
